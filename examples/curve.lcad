; Use flexible axle library function for easier creation of flexible axles.
(import flexible-axle :local)

; Create curve that defines the axle.
(def a-curve (curve ((0 0 0) (1 0 0) (0 0 1))
		    (((bw 100) 0 0) (1 0 0))))

; Draw the axle.
(flexible-axle 9 a-curve 7)
	       
; Reference length object.
(import locate :local)
(tb 3.5 0.2 0 0 0 0 "60485" 0)
